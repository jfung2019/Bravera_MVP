<h2>Show Team</h2>

<ul>

  <li>
    <strong>Activity:</strong>
    <%= @team.activity %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @team.name %>
  </li>

  <li>
    <strong>Location:</strong>
    <%= @team.location %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.team_path(@conn, :edit, @team) %></span>
<span><%= link "Back", to: Routes.team_path(@conn, :index) %></span>
