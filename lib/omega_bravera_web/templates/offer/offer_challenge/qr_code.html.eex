
<%= if @challenge.status == "complete" do %>
<div class="card challenge-details">
    <div class="m-3 text-justify challenge-desc mx-auto">
        <div class=" img-fluid margin-auto mb-3">
            <%= raw get_qr_code(@conn, @current_user, @challenge) %>
        </div>
    </div>
</div>
<% end %>
